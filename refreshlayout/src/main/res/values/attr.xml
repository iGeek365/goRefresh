<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GoRefreshLayout">
        <attr name="maxHeight" format="dimension"/>
        <attr name="headerHeight" format="dimension"/>
        <attr name="footerHeight" format="dimension"/>
        <attr name="refreshHeight" format="dimension"/>
        <attr name="damping" format="float"/>
        <attr name="isFixed" format="boolean"/>
        <attr name="duration_BacktorefreshHeight" format="integer"/>
        <attr name="duration_BacktoTop" format="integer"/>
        <attr name="duration_FooterVisibility" format="integer"/>
        <attr name="duration_FooterHidden" format="integer"/>
        <attr name="duration_autotoRefreshHeight" format="integer"/>
    </declare-styleable>


    <declare-styleable name="RingProgressBar">
        <attr name="progress_ball_color" format="color" />
        <attr name="progress_ring_color" format="color" />
        <attr name="progress_ball_radius" format="dimension" />
        <attr name="progress_ring_radius" format="dimension" />
        <attr name="progress_ring_stokewidth" format="dimension" />
        <attr name="progress_duration" format="integer" />
    </declare-styleable>
</resources>